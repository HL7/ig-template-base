      "base": "{{[type]}}-{{[id]}}.html",
      "format": "{{[type]}}-{{[id]}}.{{[fmt]}}.html",
      "list-types": "CodeSystem|ValueSet",
      "template-change-history": "template/layouts/layout-changehistory.html",
