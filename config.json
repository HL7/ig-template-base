      "profile-xml": "{{[type]}}-{{[id]}}.profile.xml.html",
      "profile-json": "{{[type]}}-{{[id]}}.profile.json.html",
      "profile-ttl": "{{[type]}}-{{[id]}}.profile.ttl.html",
      "profile-history": "{{[type]}}-{{[id]}}.profile.history.html",
